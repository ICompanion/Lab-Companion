<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane minWidth="-Infinity" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AnalysisOverviewController">
   <center>
      <VBox fx:id="analysisVBox" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="patientIdLabel" alignment="TOP_CENTER" text="Patient : " />
            <Label fx:id="analysisLabel" alignment="TOP_CENTER" text="Analyse : " />
            <Button fx:id="backButton" contentDisplay="CENTER" mnemonicParsing="false" text="Retour" />
         </children>
      </HBox>
   </top>
</BorderPane>
