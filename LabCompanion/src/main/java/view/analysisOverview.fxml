<BorderPane minHeight="800.0" minWidth="600.0" prefWidth="149.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AnalysisOverviewController">
   <center>
      <VBox fx:id="analysisVBox" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="analysisTable" prefHeight="775.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="nameColumn" maxWidth="2000" minWidth="150" prefWidth="150" style="-fx-alignment: CENTER;" text="Nom" />
                <TableColumn fx:id="minValueColumn" maxWidth="2000" minWidth="150" prefWidth="150" style="-fx-alignment: CENTER;" text="Valeur Min" />
                  <TableColumn fx:id="maxValueColumn" maxWidth="2000" minWidth="150" prefWidth="150" style="-fx-alignment: CENTER;" text="Valeur Max" />
                  <TableColumn fx:id="valueColumn" maxWidth="2000" minWidth="150" prefWidth="150" style="-fx-alignment: CENTER;" text="Valeur" />
                  <TableColumn fx:id="unitColumn" maxWidth="2000" minWidth="150" prefWidth="150" style="-fx-alignment: CENTER;" text="UnitÃ©" />
              </columns>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TableView>
         </children></VBox>
   </center>
   <top>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="patientIdLabel" styleClass="title_label" text="Patient:">
               <padding>
                  <Insets right="5.0" />
               </padding>
            </Label>
            <Label fx:id="codePatient" styleClass="title_value" text="CODE_PATIENT">
               <padding>
                  <Insets right="10.0" />
               </padding>
            </Label>
            <Label fx:id="analysisLabel" alignment="TOP_CENTER" styleClass="title_label" text="Analyse:">
               <padding>
                  <Insets right="5.0" />
               </padding></Label>
            <Label fx:id="codeAnalysis" alignment="TOP_CENTER" prefHeight="15.0" prefWidth="123.0" styleClass="title_value" text="CODE_ANALYSE">
               <padding>
                  <Insets right="10.0" />
               </padding></Label>
            <Button fx:id="backButton" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" styleClass="btn_primary" text="Retour" textAlignment="RIGHT" />
         </children>
      </HBox>
   </top>
</BorderPane>
